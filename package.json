{
  "name": "xsvg",
  "private": true,
  "version": "0.1.0",
  "description": "",
  "main": "dist/main.umd.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "clean": "rimraf dist",
    "build": "rimraf dist && cross-env NODE_ENV=production rollup --config build/rollup.config.base.js",
    "build:watch": "rimraf dist && cross-env NODE_ENV=development rollup --config build/rollup.config.base.js -w",
    "build:dev": "rimraf dist && cross-env NODE_ENV=development rollup --config build/rollup.config.base.js",
    "build:es": "rollup --config build/rollup.config.es.js",
    "build:umd": "rollup --config build/rollup.config.umd.js",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "stylelint": "stylelint **/*.less",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:update": "jest --updateSnapshot"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.(js|jsx|ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "keywords": [],
  "author": "Go7hic",
  "license": "ISC",
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "devDependencies": {
